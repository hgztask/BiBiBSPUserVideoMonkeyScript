{
  "@name": "哔哩哔哩屏蔽增强器",
  "alias": "BIBIShield_v2",
  "@namespace": "http://tampermonkey.net/",
  "@license": "Apache-2.0",
  "@version": "2.14",
  "@author": "byhgz",
  "@description": "对B站视频或评论进行屏蔽，支持关键词模糊正则等，支持时长播放弹幕过滤等，如视频、评论、动态、直播间的评论等，详情可看下面支持的屏蔽类型",
  "@icon": "https://static.hdslb.com/images/favicon.ico",
  "noFrames": true,
  "@run-at": "document-start",
  "@grant": [
    "GM_setValue",
    "GM_getValue",
    "GM_deleteValue",
    "GM_addStyle",
    "GM_unregisterMenuCommand",
    "GM_registerMenuCommand",
    "GM_openInTab",
    "GM_xmlhttpRequest",
    "GM_getResourceText"
  ],
  "@exclude": [
    "*://message.bilibili.com/pages/nav/header_sync",
    "*://message.bilibili.com/pages/nav/index_new_pc_sync",
    "*://live.bilibili.com/blackboard/dropdown-menu.html",
    "*://live.bilibili.com/p/html/live-web-mng/*",
    "*://www.bilibili.com/correspond/*",
    "http://localhost:3001/"
  ],
  "@match": [
    "*://search.bilibili.com/*",
    "*://t.bilibili.com/*",
    "*://space.bilibili.com/*",
    "*://live.bilibili.com/*",
    "*://www.bilibili.com/*",
    "*://localhost:5173/*"
  ],
  "@require": [
    "https://unpkg.com/vue@2.7.16/dist/vue.min.js",
    "https://unpkg.com/element-ui@2.15.14/lib/index.js",
    "https://unpkg.com/dexie@4.2.0/dist/dexie.min.js"
  ],
  "@source": "https://gitee.com/hangexi/BiBiBSPUserVideoMonkeyScript",
  "homepage": "https://scriptcat.org/zh-CN/script-show-page/1029"
}

